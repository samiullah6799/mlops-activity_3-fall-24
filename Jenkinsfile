pipeline {
    agent any
    stages {
        stage ('checkout') {
            steps {
                echo 'Checkout Repo'
            }
        }

        stage ('Build') {
            steps {
                echo 'Installing Dependencies'
            }
        }

        stage ('Testing') {
            steps {
                echo 'Testing'
            }
        }

        stage ('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}